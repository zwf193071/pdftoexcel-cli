{
  "name": "pdftoexcel-cli",
  "version": "1.0.0",
  "description": "Read the words in PDF, then export them into the excel",
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/zwf193071/pdftoexcel-cli"
  },
  "engines": {
    "node": ">= 8"
  },
  "bin": {
    "pdftoexcel": "./src/index.js"
  },
  "main": "./src/index.js",
  "files": [
    "src"
  ],
  "keywords": [
    "PDF",
    "excel"
  ],
  "author": "zwf193071",
  "license": "MIT",
  "dependencies": {
    "chalk": "^3.0.0",
    "import-jsx": "^3.0.0",
    "ink": "^2.5.0",
    "inquirer": "^7.0.0",
    "meow": "^5.0.0",
    "normalize-url": "^4.4.0",
    "pretty-bytes": "^5.3.0",
    "react": "^16.11.0",
    "tinycolor2": "^1.4.1",
    "update-notifier": "^3.0.1"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "eslint-config-xo-react": "^0.20.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "execa": "^2.0.4",
    "node-xlsx": "^0.15.0",
    "pdf2json": "^1.1.8",
    "prettier": "^1.18.2",
    "semver": "^6.3.0",
    "xo": "^0.24.0"
  },
  "xo": {
    "semicolon": false,
    "prettier": true,
    "extends": "xo-react",
    "rules": {
      "react/prop-types": "off",
      "react/jsx-fragments": "off",
      "react/jsx-curly-newline": "off"
    }
  },
  "prettier": {
    "useTabs": true,
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "proseWrap": "always",
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "useTabs": false
        }
      }
    ]
  }
}
